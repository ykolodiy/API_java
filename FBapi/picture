import com.restfb.DefaultFacebookClient;
import com.restfb.FacebookClient;
import com.restfb.Parameter;
import com.restfb.json.JsonObject;

public class picture {

	public static void main(String[] args) {
		String accessToken = "EAACEdEosecBAMKvnZBnKGDNNtZCdveq57OIGtWk2d0axnByZAhKXIEiHVQur75BUAeCfWehwsZCL7hifiXwTiYaLYphBVyZCqxZBuw7vHaDRZAZAgq7WyVYzRyRcqeBkfZB9w0EviCHDLOoc7urhFhGt9fhADCo7ARZCW9BkL6Fq4jW6gV7hwyrWVNzlkIJghNjEFqNZBCfOq0vwZDZD";
		
		FacebookClient client = new DefaultFacebookClient(accessToken);
		JsonObject picture = 
		      client.fetchObject("me/picture", 
			      JsonObject.class, Parameter.with("redirect","false"));
		
		
		
		System.out.println(picture.toString());
		
		JsonObject data = picture.getJsonObject("data");
		String url = data.getString("url");
		System.out.println(url);
	}
	

}
